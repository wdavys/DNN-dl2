        "  else:\n",
        "    X_train = X_train_dnn[:nb_data]\n",
        "    y_train = y_train_dnn[:nb_data]\n",
        "  \n",
        "  if pre_train:\n",
        "    dbn = DBN(dim_layers)\n",
        "    _ = dbn.pretrain(X_train, nb_epoch=nb_epoch, epoch_per_plot=10, batch_size=13, lr=1e-1, height=28, width=28, verbose=False)\n",
        "    dnn = DNN(n_classes, dbn=dbn1)\n",
        "  else:\n",
        "    dnn = DNN(n_classes, dim_layers)\n",
